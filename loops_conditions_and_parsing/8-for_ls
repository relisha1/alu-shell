#!/usr/bin/env bash
# This script displays the content of the current directory in a list format,
# showing only the part of the name after the first dash.

for file in *; do
  # Skip hidden files and check if the file contains a dash
  if [[ "$file" == *-* ]]; then
    # Extract the part after the first dash
    echo "${file#*-}"
  else
    # Display the filename if it doesn't contain a dash
    echo "$file"
  fi
done

