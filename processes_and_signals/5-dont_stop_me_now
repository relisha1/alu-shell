#!/usr/bin/env bash
# This script stops the '4-to_infinity_and_beyond' process using the kill command.

# Find the PID of the '4-to_infinity_and_beyond' process using a precise search.
pid=$(ps -eo pid,cmd | grep '[4]-to_infinity_and_beyond' | awk '{print $1}')

# Check if the PID was found.
if [ -n "$pid" ]; then
  kill "$pid" && echo "Process 4-to_infinity_and_beyond (PID: $pid) has been terminated."
else
  echo "No running '4-to_infinity_and_beyond' process found."
fi

